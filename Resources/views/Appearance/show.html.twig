<h1>Appearance</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>                <td>{{ entity.id }}</td>        </tr>
        <tr>
            <th>Basemodel</th>                <td>{{ entity.baseModel }}</td>        </tr>
        <tr>
            <th>Scale</th>                <td>{{ entity.scale }}</td>        </tr>
        <tr>
            <th>Bitmask</th>                <td>{{ entity.bitMask }}</td>        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('appearance') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('appearance_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('appearance_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
